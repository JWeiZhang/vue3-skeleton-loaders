{
  "name": "vue3-skeleton-loaders",
  "description": "This component is a versatile tool that can fill many roles within a project. At its heart, the component provides an indication to the user that something is coming but not yet available.",
  "author": "JWeiZhang <howard83124@gmail.com>",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-skeleton-loaders.umd.js",
  "module": "./dist/vue3-skeleton-loaders.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-skeleton-loaders.es.js",
      "require": "./dist/vue3-skeleton-loaders.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/components dist/types",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.26",
    "sass": "^1.45.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.30.0"
  },
  "keywords": [
    "vue3",
    "skeleton",
    "loaders",
    "骨架屏"
  ]
}
